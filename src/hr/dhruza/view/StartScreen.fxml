<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.dhruza.controller.StartScreenController">
   <children>
      <Button fx:id="btnStart" layoutX="75.0" layoutY="264.0" mnemonicParsing="false" onAction="#onStartButtonClick" prefHeight="49.0" prefWidth="150.0" text="Start New Game" />
      <Button fx:id="btnLoad" layoutX="375.0" layoutY="264.0" mnemonicParsing="false" onAction="#onStartButtonClick" prefHeight="49.0" prefWidth="150.0" text="Load Previous Game" />
      <ComboBox fx:id="cbPlayers" layoutX="191.0" layoutY="158.0" onAction="#onPlayerCountSelectionChange" prefHeight="25.0" prefWidth="198.0" />
      <Label layoutX="231.0" layoutY="121.0" text="Set number of players" />
   </children>
</AnchorPane>
